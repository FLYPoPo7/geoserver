<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <!-- 在左侧菜单栏添加一个菜单按钮  -->
    <bean id="helloPage" class="org.geoserver.web.MenuPageInfo">
        <property name="id" value="helloPage"/>
        <property name="titleKey" value="HelloPage.page.title"/>
        <property name="descriptionKey" value="HelloPage.page.description"/>
        <property name="componentClass" value="org.flypopo.geoserver.HelloPage"/>
        <property name="category" ref="dataCategory" />
        <property name="order" value="100" />
<!--        <property name="icon" value="../../img/icons/silk/database.png" />-->
        <property name="authorizer" ref="workspaceAdminAuthorizer"/>
    </bean>
    <bean id="cesiumPreviewPage" class="org.geoserver.web.MenuPageInfo">
        <property name="id" value="cesiumPreviewPage"/>
        <property name="titleKey" value="CesiumPreviewPage.page.title"/>
        <property name="descriptionKey" value="CesiumPreviewPage.page.description"/>
        <property name="componentClass" value="org.flypopo.geoserver.CesiumPreviewPage"/>
        <property name="authorizer" ref="workspaceAdminAuthorizer"/>
    </bean>
</beans>